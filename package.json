{
  "name": "my-chore-tracker",
  "version": "1.0.0",
  "description": "A MERN stack application for tracking family chores",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-server": "cd server && npm install",
    "install-client": "cd client && npm install",
    "install": "npm run install-server && npm run install-client",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "assistant:start": "node server/assistant-server.js",
    "assistant:dev": "nodemon server/assistant-server.js",
    "assistant:both": "concurrently \"npm run server\" \"npm run assistant:dev\"",
    "assistant:all": "concurrently \"npm run server\" \"npm run client\" \"npm run assistant:dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^6.2.0",
    "http-proxy-middleware": "^3.0.2",
    "nodemon": "^2.0.12"
  },
